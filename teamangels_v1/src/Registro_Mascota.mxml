			//SE VALIDAN LOS CAMPOS OBLIGATORIOS
			private function ValidarTodo():Boolean{
				
				var Nombre_Mascota:ValidationResultEvent=ValidacionNombre_Mascota.validate();
				var Especie:ValidationResultEvent=Validacion_Especie.validate();
				var Raza:ValidationResultEvent=Validacion_Raza.validate();
				var Genero:ValidationResultEvent=Validacion_Genero.validate();
				var Estado_Reproductivo:ValidationResultEvent=Validacion_Estado_Reproductivo.validate();
				var Tamano:ValidationResultEvent=Validacion_Estado_Tamano.validate();
				var Tipo_Alimentacion:ValidationResultEvent=Validacion_Tipo_Alimentacion.validate();
				
				
				var rsp:Boolean=false;
				
				if( 
					Nombre_Mascota.type==ValidationResultEvent.VALID
					&& Especie.type==ValidationResultEvent.VALID
					&& Raza.type==ValidationResultEvent.VALID
					&& Genero.type==ValidationResultEvent.VALID
					&& Estado_Reproductivo.type==ValidationResultEvent.VALID
					&& Tamano.type==ValidationResultEvent.VALID
					&& Tipo_Alimentacion.type==ValidationResultEvent.VALID
				
					
				){
					rsp=true;
				}else{
					rsp=false;
				}
				return rsp;
			}		
			
			
			
